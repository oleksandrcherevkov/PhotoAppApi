{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=(localdb)\\mssqllocaldb;Database=PhotoAppDb;Trusted_Connection=True;"
  },
  "Files": {
    "PostPhotoMaxSize": "5242880",
    "AvatarMaxSize": "524288"
  },
  "EmailSender": {
    "Email": "email",
    "SmtpServer": "smtp"
  },
  "KeyVault": {
    "KVUrl": "url",
    "TenantId": "guid",
    "ClientId": "guid",
    "ClientSecret": "key"
  },
  "AuthOptions": {
    "ISSUER": "MyAuthServer",
    "AUDIENCE": "MyAuthClient",
    "KEY": "key", 
    "LIFETIME": 1
  },
  "WebAppLoginUrl": "http://localhost:3000/login"
}
